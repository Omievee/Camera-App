<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/black">


    <FrameLayout
            android:id="@+id/fragContainerB"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />



    <com.itsovertime.overtimecamera.play.viewpager.CustomViewpager
            android:id="@+id/viewPager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible" />


    <com.itsovertime.overtimecamera.play.onboarding.PhoneVerificationView
            android:id="@+id/phoneVerificationView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />


    <com.itsovertime.overtimecamera.play.permissions.PermissionsView
            android:id="@+id/permissions"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone" />


</androidx.constraintlayout.widget.ConstraintLayout>